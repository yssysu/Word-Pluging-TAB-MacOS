<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="TaskPaneApp">
  <Id>09115ce6-1efb-42ed-a233-d6c9fbd1698d</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>718</ProviderName>
  <DefaultLocale>zh-CN</DefaultLocale>
  <DisplayName DefaultValue="Word多标签编辑器"/>
  <Description DefaultValue="在Word中实现多标签页编辑功能"/>
  <IconUrl DefaultValue="https://yssysu.github.io/Word-Pluging-TAB-MacOS/Tabs/发布xml/assets/icon-32.png"/>
  <HighResolutionIconUrl DefaultValue="https://yssysu.github.io/Word-Pluging-TAB-MacOS/Tabs/发布xml/assets/icon-64.png"/>
  <SupportUrl DefaultValue="https://github.com/yssysu/Word-Pluging-TAB-MacOS/issues"/>
  <AppDomains>
    <AppDomain>yssysu.github.io</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Document"/>
  </Hosts>
  <DefaultSettings>
    <SourceLocation DefaultValue="https://yssysu.github.io/Word-Pluging-TAB-MacOS/Tabs/发布xml/taskpane.html"/>
  </DefaultSettings>
  <Permissions>ReadWriteDocument</Permissions>
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
    <Hosts>
      <Host xsi:type="Document">
        <DesktopFormFactor>
          <GetStarted>
            <Title resid="GetStarted.Title"/>
            <Description resid="GetStarted.Description"/>
            <LearnMoreUrl resid="GetStarted.LearnMoreUrl"/>
          </GetStarted>
          <FunctionFile resid="Commands.Url"/>
          <ExtensionPoint xsi:type="PrimaryCommandSurface">
            <OfficeTab id="TabHome">
              <Group id="CommandsGroup">
                <Label resid="CommandsGroup.Label"/>
                <Icon>
                  <bt:Image size="16" resid="Icon.16x16"/>
                  <bt:Image size="32" resid="Icon.32x32"/>
                  <bt:Image size="80" resid="Icon.80x80"/>
                </Icon>
                <Control xsi:type="Button" id="TaskpaneButton">
                  <Label resid="TaskpaneButton.Label"/>
                  <Supertip>
                    <Title resid="TaskpaneButton.Label"/>
                    <Description resid="TaskpaneButton.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.16x16"/>
                    <bt:Image size="32" resid="Icon.32x32"/>
                    <bt:Image size="80" resid="Icon.80x80"/>
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <TaskpaneId>ButtonId1</TaskpaneId>
                    <SourceLocation resid="Taskpane.Url"/>
                  </Action>
                </Control>
              </Group>
            </OfficeTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    <Resources>
      <bt:Images>
        <bt:Image id="Icon.16x16" DefaultValue="https://yssysu.github.io/Word-Pluging-TAB-MacOS/Tabs/发布xml/assets/icon-16.png"/>
        <bt:Image id="Icon.32x32" DefaultValue="https://yssysu.github.io/Word-Pluging-TAB-MacOS/Tabs/发布xml/assets/icon-32.png"/>
        <bt:Image id="Icon.80x80" DefaultValue="https://yssysu.github.io/Word-Pluging-TAB-MacOS/Tabs/发布xml/assets/icon-80.png"/>
      </bt:Images>
      <bt:Urls>
        <bt:Url id="GetStarted.LearnMoreUrl" DefaultValue="https://github.com/yssysu/Word-Pluging-TAB-MacOS"/>
        <bt:Url id="Commands.Url" DefaultValue="https://yssysu.github.io/Word-Pluging-TAB-MacOS/Tabs/发布xml/commands.html"/>
        <bt:Url id="Taskpane.Url" DefaultValue="https://yssysu.github.io/Word-Pluging-TAB-MacOS/Tabs/发布xml/taskpane.html"/>
      </bt:Urls>
      <bt:ShortStrings>
        <bt:String id="GetStarted.Title" DefaultValue="Word多标签编辑器"/>
        <bt:String id="CommandsGroup.Label" DefaultValue="多标签编辑"/>
        <bt:String id="TaskpaneButton.Label" DefaultValue="打开多标签"/>
      </bt:ShortStrings>
      <bt:LongStrings>
        <bt:String id="GetStarted.Description" DefaultValue="您的多标签编辑器已加载成功。点击'HOME'选项卡中的'打开多标签'按钮开始使用。"/>
        <bt:String id="TaskpaneButton.Tooltip" DefaultValue="点击打开多标签编辑器"/>
      </bt:LongStrings>
    </Resources>
  </VersionOverrides>
</OfficeApp>